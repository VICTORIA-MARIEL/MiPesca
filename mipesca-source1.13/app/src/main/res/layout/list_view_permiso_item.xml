<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cardView"
    android:layout_width="match_parent"
    android:layout_height="200dp"
    android:layout_margin="@dimen/activity_horizontal_margin"
    android:clickable="true"
    android:focusable="true"
    android:foreground="?android:attr/selectableItemBackground"
    card_view:cardCornerRadius="4dp">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:orientation="vertical">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:src="@drawable/outline_assignment_24"
            android:visibility="gone"/>
        <TextView
            android:id="@+id/lblPermisoNumero"
            style="@style/midetiqueta"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:text="@string/lblListadoPermisoNumero"/>
        <TextView
            android:id="@+id/lblPermisoParaPesqueria"
            style="@style/midetiqueta"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:text="@string/lblListadoPermisoParaPesqueria"/>
        <TextView
            android:id="@+id/lblPermisoEstatus"
            style="@style/midetiqueta"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textAlignment="center"
            android:text="@string/lblListadoPermisoEstatus"/>

    </LinearLayout>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/txtPermisoNumero"
            style="@style/midtexto"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:text="125080997113"
            />


        <TextView
            android:id="@+id/txtPermisoParaPesqueria"
            style="@style/midtexto"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:text="TUNIDOS"
            />
        <TextView
            android:id="@+id/txtPermisoEstatus"
            style="@style/midtexto"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textAlignment="center"
            android:text="INACTIVO"/>

    </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:background="#ffffff"/>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/lblPermisoLugarExpedicion"
            style="@style/midetiqueta"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:text="@string/lblListadoPermisoLugarExpedicion"/>

        <TextView
            android:id="@+id/lblPermisoFhExpedicion"
            style="@style/midetiqueta"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textAlignment="center"
            android:text="@string/lblListadoPermisoFhExpedicion"/>

    </LinearLayout>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/txtPermisoLugarExpedicion"
            style="@style/midtexto"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="italic"
            android:text="MAZATLAN, SINALOA"

            />
        <TextView
            android:id="@+id/txtPermisoFhExpedicion"
            style="@style/midtexto"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="italic"
            android:textAlignment="center"
            android:text="12 DE DICIEMBRE DE 2017"
            />
        >
    </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:background="#ffffff"/>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/lblPermisoFhVigenciaDuracion"
            style="@style/midetiqueta"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textAlignment="textStart"
            android:text="@string/lblPermisoFhVigenciaDuracion"/>
        <TextView
            android:id="@+id/lblPermisoFhVigenciaInicio"
            style="@style/midetiqueta"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textAlignment="center"
            android:text="@string/lblPermisoFhVigenciaInicio"/>

        <TextView
            android:id="@+id/lblPermisoFhVigenciaFin"
            style="@style/midetiqueta"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textAlignment="center"
            android:text="@string/lblPermisoFhVigenciaFin"/>
    </LinearLayout>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/txtPermisoFhVigenciaDuracion"
            style="@style/midtexto"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="italic"
            android:textAlignment="textStart"
            android:text="3 AÃ‘OS"
            />

        <TextView
            android:id="@+id/txtPermisoFhVigenciaInicio"
            style="@style/midtexto"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="italic"
            android:textAlignment="center"
            android:text="20/12/2017"
            />
        <TextView
            android:id="@+id/txtPermisoFhVigenciaFin"
            style="@style/midtexto"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="italic"
            android:textAlignment="center"
            android:text="20/12/2020"
            />

    </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:background="#ffffff"/>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
android:layout_gravity="right"
        >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_weight="0.1"
            android:src="@drawable/baseline_search_24" />
    </LinearLayout>
    </LinearLayout>
</android.support.v7.widget.CardView>
