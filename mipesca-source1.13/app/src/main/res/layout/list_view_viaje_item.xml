<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cardView"
    android:layout_width="match_parent"
    android:layout_height="200dp"
    android:layout_margin="@dimen/activity_horizontal_margin"
    android:clickable="true"
    android:focusable="true"
    android:foreground="?android:attr/selectableItemBackground"
    card_view:cardCornerRadius="4dp">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:orientation="vertical">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal">
        <TextView
            android:id="@+id/lblViajeFhRegistro"
            style="@style/midetiqueta"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"

            android:text="@string/lblListadoViajeFhRegistro"/>
        <TextView
            android:id="@+id/lblViajeFolio"
            style="@style/midetiqueta"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textAlignment="center"
            android:text="@string/lblListadoViajeFolio"/>
        <TextView
            android:id="@+id/lblViajeEstatus"
            style="@style/midetiqueta"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textAlignment="center"
            android:text="@string/lblListadoViajeEstatus"/>


    </LinearLayout>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal">
        <TextView
            android:id="@+id/txtViajeFhRegistro"
            style="@style/midtexto"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="italic"

            android:text="17/07/2019"
            />
        <TextView
            android:id="@+id/txtViajeFolio"
            style="@style/midtexto"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textAlignment="center"
            android:textStyle="italic"
            android:text="F19123-1"
            />
        <TextView
            android:id="@+id/txtViajeEstatus"
            style="@style/midtexto"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="italic"
            android:textAlignment="center"
            android:text="INACTIVO"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#ffffff"/>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal">
        <TextView
            android:id="@+id/lblViajeComunidad"
            style="@style/midetiqueta"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:text="@string/lblListadoViajeComunidad"/>
        <TextView
            android:id="@+id/lblViajeArtePesca"
            style="@style/midetiqueta"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:text="@string/lblListadoViajeArtePesca"/>


    </LinearLayout>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal">
        <TextView
            android:id="@+id/txtViajeComunidad"
            style="@style/midtexto"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="italic"
            android:text="GOLFO DE CALIFORNIA"
            />

        <TextView
            android:id="@+id/txtViajeArtePesca"
            style="@style/midtexto"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="italic"
            android:text="PROCESO MANUAL"
            />

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#ffffff"/>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal">
        <TextView
            android:id="@+id/lblViajeCombustible"
            style="@style/midetiqueta"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:text="@string/lblListadoViajeCombustible"/>

        <TextView
            android:id="@+id/lblViajeRecursos"
            style="@style/midetiqueta"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:text="@string/lblListadoViajeRecursos"/>


    </LinearLayout>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal">
        <TextView
            android:id="@+id/txtViajeCombustible"
            style="@style/midtexto"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="italic"
            android:text="20 litros"
            />
        >


        <TextView
            android:id="@+id/txtViajeRecursos"
            style="@style/midtexto"
            android:layout_weight="1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="italic"
            android:text="12 Recursos"
            />
    </LinearLayout>


    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="right"
        >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_weight="0.1"
            android:src="@drawable/baseline_search_24" />
    </LinearLayout>
    </LinearLayout>
</android.support.v7.widget.CardView>
